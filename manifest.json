{
    "manifest_version": 3,
    "name": "extension",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": ["scripting"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "content_scripts": [
        {"matches": ["<all_urls>"], "js": ["src/content/content.js"]}
    ]
}